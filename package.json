    "db:studio": "dotenv -e .env.development -- drizzle-kit studio",
    "db:seed": "dotenv -e .env.development -- tsx ./src/server/db/seed/seedData.ts",
    "db:seed:prod": "npx --yes tsx ./src/server/db/seed/seedData.ts",
    "db:up": "docker-compose up portfolio-db -d",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  },
